{
  "name": "pde_cfe",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start:debug": "nodemon --inspect src/server.js",
    "build": "sucrase ./src -d ./dist --transforms imports",
    "start": "node dist/server.js",
    "queue": "nodemon src/queue.js",
    "install-db-postgre": "docker run --name db_postgre -e POSTGRES_PASSWORD=root -p 5432:5432 -d -t postgres",
    "install-db-mongo": "docker run --name db_mongo  -p 27017:27017 -d -t mongo",
    "install-db-redis": "docker run --name db_redis -p 6379:6379 -d -t redis:alpine",
    "eslint-fix": "yarn eslint --fix src --ext .js",
    "migrate-create": "yarn sequelize migration:create --name=users",
    "migrate": "yarn sequelize db:migrate",
    "migrate-undo": "yarn sequelize db:migrate:undo",
    "migrate-undo-all": "yarn sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "@sentry/node": "5.14.2",
    "bcryptjs": "^2.4.3",
    "bee-queue": "^1.2.3",
    "cors": "^2.8.5",
    "date-fns": "^2.11.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.5",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.16",
    "nodemailer-express-handlebars": "^3.3.0",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.5",
    "youch": "^2.0.10",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "sucrase": "^3.12.1"
  }
}
